@model ErrorViewModel
@{
    ViewData["title"] = $"Something went wrong";
}
<section class="ftco-section ftco-services-2">
    <div class="container">
        <div class="row">
            <div class="col-md-12 text-center mb-md-5 mt-md-5">
                <h1>Oh no! Something went wrong</h1>
                <h3>I can't found what you're looking for</h3>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12 property-wrap mb-2">
                <div class="row">
                    <div class="col-md-12 ftco-animate">
                        <h5 class="mb-4">
                            Please have a look at our <a href="@Url.Action("Search", "Property")">listings</a> or contact me using the form below to get help with your search!
                        </h5>
                        <div>
                            @await Html.PartialAsync("_RequestInformationForm", new RequestInformationModel())
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section> <!-- .section -->