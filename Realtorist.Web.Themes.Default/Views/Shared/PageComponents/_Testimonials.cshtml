@model Dictionary<string, JToken>
@{
    var testimonials = Model?.GetValueOrDefault("testimonials")?.ToObject<Testimonial[]>() ?? new Testimonial[0];
}
<section class="ftco-section testimony-section testimonials-component">
    <div class="container">
        <div class="row justify-content-center pb-3 title-row">
            <div class="col-md-7 text-center heading-section heading-section-white ftco-animate">
                <span class="subheading">@(Model?.GetValueOrDefault("title")?.ToObject<string>() ?? "Read testimonials")</span>
                <h2 class="mb-4">@(Model?.GetValueOrDefault("subTitle")?.ToObject<string>() ?? "What Client Says")</h2>
            </div>
        </div>
        <div class="row ftco-animate justify-content-center">
            <div class="col-md-12">
                <div class="carousel-testimony owl-carousel ftco-owl">
                    @foreach (var testimonial in testimonials.Take(10))
                    {
                        <div class="item">
                            <div class="testimony-wrap text-center py-4 pb-5">
                                <div class="user-img"
                                 style="background-image: url(@testimonial.Photo)">
                                    <span class="quote d-flex align-items-center justify-content-center">
                                        <i class="icon-quote-left"></i>
                                    </span>
                                </div>
                                <div class="text px-4 pb-5">
                                    <p class="mb-4">@testimonial.Text</p>
                                    <p class="name">@testimonial.Name</p>
                                    <span class="position">@testimonial.Title</span>
                                </div>
                            </div>
                        </div>
                    }
                </div>
            </div>
        </div>
    </div>
</section>